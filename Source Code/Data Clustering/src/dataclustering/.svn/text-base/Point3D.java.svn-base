public class Point3D {

    double x;
    double y;
    double z;

    Point3D(double x, double y, double z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    Point3D(Point3D newPoint) {
        this.x = newPoint.x;
        this.y = newPoint.y;
        this.z = newPoint.z;
    }
}

class Anobject{
    public double x;
    public double y;
    public double z;
    public int center;
    public int cluster;
    public Anobject(double x, double y, double z){
        this.x = x;
        this.y = y;
        this.z = z;

    }

}
